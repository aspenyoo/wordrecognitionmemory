%% checking to see SD of nLLs for FP model
% 6/29/2016

modelname = 'FP';
nSamples = 100;

% simulate data
[resp, tp] = simulate_resp(modelname,1,1,[])

nLLVec = nan(1,nSamples);
for isample = 1:nSamples
    isample
    nLLVec(isample) = nLL_approx_vectorized(modelname,tp,1,resp.new,resp.old);
end

hist(nLLVec)
mean(nLLVec)
std(nLLVec)